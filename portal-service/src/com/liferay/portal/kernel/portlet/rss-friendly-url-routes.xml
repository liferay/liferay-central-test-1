<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.0.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_0_0.dtd">

<routes>
	<route>
		<pattern>/rss</pattern>
		<default-parameter name="rss">true</default-parameter>
		<overridden-parameter name="p_p_state">exclusive</overridden-parameter>
	</route>
	<route>
		<pattern>/rss</pattern>
		<default-parameter name="rss">t</default-parameter>
		<overridden-parameter name="p_p_state">exclusive</overridden-parameter>
	</route>
	<route>
		<pattern>/rss</pattern>
		<default-parameter name="rss">y</default-parameter>
		<overridden-parameter name="p_p_state">exclusive</overridden-parameter>
	</route>
	<route>
		<pattern>/rss</pattern>
		<default-parameter name="rss">on</default-parameter>
		<overridden-parameter name="p_p_state">exclusive</overridden-parameter>
	</route>
	<route>
		<pattern>/rss</pattern>
		<default-parameter name="rss">1</default-parameter>
		<overridden-parameter name="p_p_state">exclusive</overridden-parameter>
	</route>
</routes>