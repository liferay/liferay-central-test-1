<?xml version="1.0"?>
<!DOCTYPE plugin-package PUBLIC "-//Liferay//DTD Plugin Package 4.3.0//EN" "http://www.liferay.com/dtd/liferay-plugin-package_4_3_0.dtd">

<plugin-package>
	<name>@theme_name@ Theme</name>
	<module-id>@module_group_id@/@module_artifact_id@/@module_version@/war</module-id>
	<types>
		<type>theme</type>
	</types>
	<tags>@tags@</tags>
	<short-description>@short_description@</short-description>
	<change-log>@change_log@</change-log>
	<page-url>@page_url@</page-url>
	<author>@author@</author>
	<licenses>@licenses@</licenses>
	<liferay-versions>
		<liferay-version>@liferay_version@</liferay-version>
	</liferay-versions>
</plugin-package>