<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 5.1.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_5_1_0.dtd">

<service-builder package-path="com.liferay.wsrp">
	<namespace>WSRP</namespace>
	<entity name="WSRPConfiguredProducer" local-service="true" remote-service="false">

		<!-- PK fields -->

		<column name="configuredProducerId" type="long" primary="true" />

		<!-- Other fields -->

		<column name="name" type="String" />
		<column name="portalId" type="String" />
		<column name="namespace" type="String" />
		<column name="producerURL" type="String" />
		<column name="producerVersion" type="String" />
		<column name="producerMarkupURL" type="String" />
		<column name="status" type="int" />
		<column name="registrationData" type="String" />
		<column name="registrationContext" type="String" />
		<column name="serviceDescription" type="String" />
		<column name="userCategoryMapping" type="String" />
		<column name="customUserProfile" type="String" />
		<column name="identityPropagationType" type="String" />
		<column name="sdLastModified" type="long" />
		<column name="entityVersion" type="int" />

		<!-- Finder methods -->

		<finder name="P_N" return-type="Collection">
			<finder-column name="portalId" />
			<finder-column name="namespace" />
		</finder>

		<!-- References -->

		<reference package-path="com.liferay.counter" entity="Counter" />
	</entity>
	<entity name="WSRPPortlet" local-service="true" remote-service="false">

		<!-- PK fields -->

		<column name="portletId" type="long" primary="true" />

		<!-- Other fields -->

		<column name="name" type="String" />
		<column name="channelName" type="String" />
		<column name="title" type="String" />
		<column name="shortTitle" type="String" />
		<column name="displayName" type="String" />
		<column name="keywords" type="String" />
		<column name="status" type="int" />
		<column name="producerEntityId" type="String" />
		<column name="consumerId" type="String" />
		<column name="portletHandle" type="String"/>
		<column name="mimeTypes" type="String"/>

		<!-- Finder methods -->

		<finder name="PortletName" return-type="WSRPPortlet">
			<finder-column name="name" />
		</finder>
		<finder name="ProducerEntityId" return-type="Collection">
			<finder-column name="producerEntityId" />
		</finder>
		<finder name="P_P" return-type="Collection">
			<finder-column name="producerEntityId" />
			<finder-column name="portletHandle" />
		</finder>

		<!-- References -->

		<reference package-path="com.liferay.counter" entity="Counter" />
	</entity>
</service-builder>