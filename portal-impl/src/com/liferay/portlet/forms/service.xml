<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_0_0.dtd">

<service-builder package-path="com.liferay.portlet.forms">
	<namespace>Forms</namespace>
	<entity name="FormStructure" uuid="true" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="id" type="long" primary="true" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="formStructureId" type="String" />
		<column name="name" type="String" />
		<column name="description" type="String" />
		<column name="xsd" type="String" />

		<!-- Finder methods -->

		<finder name="GroupId" return-type="Collection">
			<finder-column name="groupId" />
		</finder>
		<finder name="G_F" return-type="FormStructure" unique="true">
			<finder-column name="groupId" />
			<finder-column name="formStructureId" />
		</finder>

		<!-- References -->

		<reference package-path="com.liferay.portlet.expando" entity="ExpandoValue" />
	</entity>
	<entity name="FormStructureLink" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="formStructureLinkId" type="long" primary="true" />
		
		<!-- Other fields -->

		<column name="formStructureId" type="String" />
		<column name="className" type="String" />
		<column name="classPK" type="long" />
		
		<!-- Finder methods -->

		<finder name="FormStructureLinkId" return-type="FormStructureLink" unique="true">
			<finder-column name="formStructureLinkId" />
		</finder>
		<finder name="FormStructureId" return-type="Collection">
			<finder-column name="formStructureId" />
		</finder>
		<finder name="F_C_C" return-type="FormStructureLink" unique="true">
			<finder-column name="formStructureId" />		
			<finder-column name="className" />
			<finder-column name="classPK" />
		</finder>
		
		<!-- References -->

		<reference package-path="com.liferay.portlet.expando" entity="ExpandoValue" />
	</entity>
	<exceptions>
		<exception>FormDuplicateStructureElement</exception>
		<exception>FormDuplicateStructureId</exception>
		<exception>FormStructureId</exception>
		<exception>FormStructureName</exception>
		<exception>FormStructureXsd</exception>
	</exceptions>
</service-builder>