<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_0_0.dtd">

<service-builder package-path="com.liferay.portlet.shorturl">
	<namespace>ShortURL</namespace>
	<entity name="ShortURL" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="shortURLId" type="long" primary="true" />

		<!-- Audit fields -->

		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="originalURL" type="String" />
		<column name="hash" type="String" />
		<column name="descriptor" type="String" />
		
		<!-- Finder methods -->

		<finder name="OriginalURL" return-type="ShortURL" unique="true">
			<finder-column name="originalURL" />
		</finder>
		<finder name="Hash" return-type="ShortURL" unique="true">
			<finder-column name="hash" />
		</finder>
	</entity>
</service-builder>