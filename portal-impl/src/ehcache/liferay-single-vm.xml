<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ehcache.xsd" name="liferay-single-vm"  updateCheck="false" dynamicConfig="false" monitoring="off">

	<!--
	Caches in this file are designed to be unique per VM and should not be
	clustered.
	-->

	<defaultCache
		maxElementsInMemory="10000"
		eternal="false"
		timeToIdleSeconds="600"
		overflowToDisk="false"
	/>
	<cache
		name="com.liferay.portal.util.WebCachePool"
		maxElementsInMemory="1000"
		eternal="false"
	/>
</ehcache>