<?xml version="1.0" encoding="UTF-8"?>

<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
>
	<bean id="internalInvokerPortlet" class="com.liferay.portlet.InvokerPortletImpl" />
	<bean id="sunInvokerPortlet" class="com.liferay.portal.portletcontainer.WindowInvoker" />
	<bean id="com.liferay.portlet.PortletInstanceFactoryImpl" class="com.liferay.portlet.PortletInstanceFactoryImpl">
		<property name="internalInvokerPortletPrototype" ref="internalInvokerPortlet" />
		<property name="sunInvokerPortletPrototype" ref="sunInvokerPortlet" />
	</bean>
	<bean id="com.liferay.portlet.PortletInstanceFactoryUtil" class="com.liferay.portlet.PortletInstanceFactoryUtil">
		<property name="portletInstanceFactory" ref="com.liferay.portlet.PortletInstanceFactoryImpl" />
	</bean>
</beans>