<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.liferay.documentlibrary.model.Content" />
	<class name="com.liferay.documentlibrary.model.Content" table="Content">
		<id name="contentId" type="long">
			<generator class="assigned" />
		</id>
		<property name="companyId" />
		<property name="portletId" />
		<property name="groupId" />
		<property name="repositoryId" />
		<property name="path" column="path_" />
		<property name="version" />
		<property name="data" column="data_" />
		<property name="size" column="size_" />
	</class>
</hibernate-mapping>