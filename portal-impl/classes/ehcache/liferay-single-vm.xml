<!--
Caches in this file are designed to be unique per VM and should not be clustered.
-->

<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ehcache.xsd">
	<defaultCache
		maxElementsInMemory="1000"
		eternal="false"
		overflowToDisk="false"
	/>
	<cache
		name="com.liferay.portal.lastmodified.LastModifiedCSS"
		maxElementsInMemory="1000"
		eternal="false"
		timeToLiveSeconds="300"
		overflowToDisk="false"
	/>
	<cache
		name="com.liferay.portal.lastmodified.LastModifiedJavaScript"
		maxElementsInMemory="1000"
		eternal="false"
		timeToLiveSeconds="300"
		overflowToDisk="false"
	/>
	<cache
		name="com.liferay.portal.util.WebCachePool"
		maxElementsInMemory="1000"
		eternal="false"
		timeToLiveSeconds="300"
		overflowToDisk="false"
	/>
	<cache
		name="com.liferay.portal.velocity.LiferayResourceCacheUtil"
		maxElementsInMemory="1000"
		eternal="false"
		timeToLiveSeconds="300"
		overflowToDisk="false"
	/>
</ehcache>