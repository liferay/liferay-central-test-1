<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ehcache.xsd">
	<diskStore path="user.home/liferay/ehcache/hibernate"/>
	<cacheManagerEventListenerFactory class="" properties=""/>
	<cacheManagerPeerProviderFactory class="net.sf.ehcache.distribution.RMICacheManagerPeerProviderFactory" properties="peerDiscovery=automatic,multicastGroupAddress=230.0.0.1,multicastGroupPort=4446,timeToLive=1" propertySeparator=","/>
	<cacheManagerPeerListenerFactory class="net.sf.ehcache.distribution.RMICacheManagerPeerListenerFactory"/>

	<!-- 
		Hibernate will use the defaultCache unless custom configurations are
		defined below for individual domain objects, collection, queries, etc
	-->

	<defaultCache maxElementsInMemory="1000" eternal="false" overflowToDisk="true">
		<!-- Enable the following in a clustered configuration -->

		<!--<cacheEventListenerFactory class="net.sf.ehcache.distribution.RMICacheReplicatorFactory">
		<bootstrapCacheLoaderFactory class="net.sf.ehcache.distribution.RMIBootstrapCacheLoaderFactory"/>-->
	</defaultCache>
</ehcache>
