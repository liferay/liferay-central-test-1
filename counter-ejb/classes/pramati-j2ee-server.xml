<?xml version="1.0"?>
<!DOCTYPE pramati-j2ee-server PUBLIC "-//Pramati Technologies //DTD Pramati J2ee Server 5.0//EN" "http://www.pramati.com/dtd/pramati-j2ee-server_5_0.dtd">

<pramati-j2ee-server>
	<vhost-name>default</vhost-name>
	<auto-start>TRUE</auto-start>
	<realm-name />
	<ejb-module>
		<name>counter-ejb.jar</name>
		<ejb>
			<name>com_liferay_counter_service_ejb_CounterLocalServiceEJB</name>
			<max-pool-size>40</max-pool-size>
			<min-pool-size>20</min-pool-size>
			<enable-freepool>false</enable-freepool>
			<pool-waittimeout-millis>60000</pool-waittimeout-millis>
			<pk-waittimeout-millis/>
			<low-activity-interval>20</low-activity-interval>
			<is-secure>false</is-secure>
			<is-clustered>true</is-clustered>
			<jndi-name>com_liferay_counter_service_ejb_CounterLocalServiceEJB</jndi-name>
			<local-jndi-name>com_liferay_counter_service_ejb_CounterLocalServiceEJB__PRAMATI_LOCAL</local-jndi-name>
			<resource-mapping>
				<resource-name>jdbc/LiferayPool</resource-name>
				<resource-type>javax.sql.DataSource</resource-type>
				<resource-link>LiferayPool</resource-link>
			</resource-mapping>
			<resource-mapping>
				<resource-name>mail/MailSession</resource-name>
				<resource-type>javax.mail.Session</resource-type>
				<resource-link>mail/MailSession</resource-link>
			</resource-mapping>
		</ejb>
		<ejb>
			<name>com_liferay_counter_service_ejb_CounterServiceEJB</name>
			<max-pool-size>40</max-pool-size>
			<min-pool-size>20</min-pool-size>
			<enable-freepool>false</enable-freepool>
			<pool-waittimeout-millis>60000</pool-waittimeout-millis>
			<pk-waittimeout-millis/>
			<low-activity-interval>20</low-activity-interval>
			<is-secure>false</is-secure>
			<is-clustered>true</is-clustered>
			<jndi-name>com_liferay_counter_service_ejb_CounterServiceEJB</jndi-name>
			<resource-mapping>
				<resource-name>jdbc/LiferayPool</resource-name>
				<resource-type>javax.sql.DataSource</resource-type>
				<resource-link>LiferayPool</resource-link>
			</resource-mapping>
			<resource-mapping>
				<resource-name>mail/MailSession</resource-name>
				<resource-type>javax.mail.Session</resource-type>
				<resource-link>mail/MailSession</resource-link>
			</resource-mapping>
		</ejb>
	</ejb-module>
</pramati-j2ee-server>