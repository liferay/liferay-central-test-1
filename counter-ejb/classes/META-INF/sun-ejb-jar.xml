<?xml version="1.0"?>
<!DOCTYPE sun-ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Sun ONE Application Server 7.0 EJB 2.0//EN" "http://www.sun.com/software/sunone/appserver/dtds/sun-ejb-jar_2_0-0.dtd">

<sun-ejb-jar>
	<enterprise-beans>
		<ejb>
			<ejb-name>com_liferay_counter_service_ejb_CounterLocalServiceEJB</ejb-name>
			<jndi-name>ejb/liferay/CounterLocalServiceHome</jndi-name>
			<bean-pool>
				<steady-pool-size>0</steady-pool-size>
				<resize-quantity>60</resize-quantity>
				<max-pool-size>60</max-pool-size>
				<pool-idle-timeout-in-seconds>900</pool-idle-timeout-in-seconds>
			</bean-pool>
		</ejb>
		<ejb>
			<ejb-name>com_liferay_counter_service_ejb_CounterServiceEJB</ejb-name>
			<jndi-name>com_liferay_counter_service_ejb_CounterServiceEJB</jndi-name>
			<bean-pool>
				<steady-pool-size>0</steady-pool-size>
				<resize-quantity>60</resize-quantity>
				<max-pool-size>60</max-pool-size>
				<pool-idle-timeout-in-seconds>900</pool-idle-timeout-in-seconds>
			</bean-pool>
		</ejb>
	</enterprise-beans>
</sun-ejb-jar>