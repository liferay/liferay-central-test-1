<?xml version="1.0"?>
<!DOCTYPE ejb-jar PUBLIC "-//Borland Software Corporation//DTD Enterprise JavaBeans 2.0//EN" "http://www.borland.com/devsupport/appserver/dtds/ejb-jar_2_0-borland.dtd">

<ejb-jar>
	<enterprise-beans>
		<session>
			<ejb-name>com_liferay_counter_service_ejb_CounterLocalServiceEJB</ejb-name>
			<bean-home-name>com_liferay_counter_service_ejb_CounterLocalServiceEJB</bean-home-name>
			<bean-local-home-name>ejb/liferay/CounterLocalServiceHome</bean-local-home-name>
		</session>
		<session>
			<ejb-name>com_liferay_counter_service_ejb_CounterServiceEJB</ejb-name>
			<bean-home-name>com_liferay_counter_service_ejb_CounterServiceEJB</bean-home-name>
			<bean-local-home-name>DefaultLocalJndiNamecom_liferay_counter_service_ejb_CounterServiceEJB</bean-local-home-name>
		</session>
	</enterprise-beans>
	<property>
		<prop-name>ejb.default_transaction_attribute</prop-name>
		<prop-type>String</prop-type>
		<prop-value>Supports</prop-value>
	</property>
</ejb-jar>