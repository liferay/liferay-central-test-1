<?xml version="1.0"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">

<ejb-jar>
	<enterprise-beans>
		<session>
			<display-name>ReportsEntryServiceEJB</display-name>
			<ejb-name>com_ext_portlet_reports_service_ejb_ReportsEntryServiceEJB</ejb-name>
			<home>com.ext.portlet.reports.service.ejb.ReportsEntryServiceHome</home>
			<remote>com.ext.portlet.reports.service.ejb.ReportsEntryServiceEJB</remote>
			<ejb-class>com.ext.portlet.reports.service.ejb.ReportsEntryServiceEJBImpl</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Bean</transaction-type>
			<ejb-local-ref>
				<ejb-ref-name>ejb/liferay/UserLocalServiceHome</ejb-ref-name>
				<ejb-ref-type>Session</ejb-ref-type>
				<local-home>com.liferay.portal.service.ejb.UserLocalServiceHome</local-home>
				<local>com.liferay.portal.service.ejb.UserLocalServiceEJB</local>
				<ejb-link>com_liferay_portal_service_ejb_UserLocalServiceEJB</ejb-link>
			</ejb-local-ref>
			<resource-ref>
				<res-ref-name>jdbc/LiferayPool</res-ref-name>
				<res-type>javax.sql.DataSource</res-type>
				<res-auth>Container</res-auth>
				<res-sharing-scope>Shareable</res-sharing-scope>
			</resource-ref>
			<resource-ref>
				<res-ref-name>mail/MailSession</res-ref-name>
				<res-type>javax.mail.Session</res-type>
				<res-auth>Container</res-auth>
			</resource-ref>
		</session>
	</enterprise-beans>
	<assembly-descriptor>
		<method-permission>
			<unchecked />
			<method>
				<ejb-name>com_ext_portlet_reports_service_ejb_ReportsEntryServiceEJB</ejb-name>
				<method-name>*</method-name>
			</method>
		</method-permission>
	</assembly-descriptor>
</ejb-jar>