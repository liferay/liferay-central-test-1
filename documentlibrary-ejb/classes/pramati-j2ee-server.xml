			<ejb-local-ref>
				<ejb-ref-name>ejb/liferay/DLFolderLocalServiceHome</ejb-ref-name>
				<ejb-link>com_liferay_portlet_documentlibrary_service_ejb_DLFolderLocalServiceEJB__PRAMATI_LOCAL</ejb-link>
			</ejb-local-ref>
			<resource-mapping>
				<resource-name>jdbc/LiferayPool</resource-name>
				<resource-type>javax.sql.DataSource</resource-type>
				<resource-link>LiferayPool</resource-link>
			</resource-mapping>
			<resource-mapping>
				<resource-name>mail/MailSession</resource-name>
				<resource-type>javax.mail.Session</resource-type>
				<resource-link>mail/MailSession</resource-link>
			</resource-mapping>
		</ejb>
		<ejb>
			<name>com_liferay_documentlibrary_service_ejb_DLServiceEJB</name>
			<max-pool-size>40</max-pool-size>
			<min-pool-size>20</min-pool-size>
			<enable-freepool>false</enable-freepool>
			<pool-waittimeout-millis>60000</pool-waittimeout-millis>
