<?xml version="1.0"?>
<!DOCTYPE pramati-j2ee-server PUBLIC "-//Pramati Technologies //DTD Pramati J2ee Server 5.0//EN" "http://www.pramati.com/dtd/pramati-j2ee-server_5_0.dtd">

<pramati-j2ee-server>
	<vhost-name>default</vhost-name>
	<auto-start>TRUE</auto-start>
	<realm-name />
	<ejb-module>
		<name>documentlibrary-ejb.jar</name>
		<ejb>
			<name>com_liferay_documentlibrary_service_ejb_DLLocalServiceEJB</name>
			<max-pool-size>40</max-pool-size>
			<min-pool-size>20</min-pool-size>
			<enable-freepool>false</enable-freepool>
			<pool-waittimeout-millis>60000</pool-waittimeout-millis>
			<pk-waittimeout-millis/>
			<low-activity-interval>20</low-activity-interval>
			<is-secure>false</is-secure>
			<is-clustered>true</is-clustered>
			<jndi-name>com_liferay_documentlibrary_service_ejb_DLLocalServiceEJB</jndi-name>
			<local-jndi-name>com_liferay_documentlibrary_service_ejb_DLLocalServiceEJB__PRAMATI_LOCAL</local-jndi-name>
			<ejb-local-ref>
				<ejb-ref-name>ejb/liferay/SubscriptionLocalServiceHome</ejb-ref-name>
				<ejb-link>com_liferay_portal_service_ejb_SubscriptionLocalServiceEJB__PRAMATI_LOCAL</ejb-link>
			</ejb-local-ref>
			<ejb-local-ref>
				<ejb-ref-name>ejb/liferay/DLFileEntryLocalServiceHome</ejb-ref-name>
				<ejb-link>com_liferay_portlet_documentlibrary_service_ejb_DLFileEntryLocalServiceEJB__PRAMATI_LOCAL</ejb-link>
			</ejb-local-ref>
			<ejb-local-ref>
				<ejb-ref-name>ejb/liferay/DLFileRankLocalServiceHome</ejb-ref-name>
