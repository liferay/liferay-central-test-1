<?xml version="1.0"?>
<!DOCTYPE weblogic-ejb-jar PUBLIC "-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB//EN" "http://www.bea.com/servers/wls810/dtd/weblogic-ejb-jar.dtd">

<weblogic-ejb-jar>
	<weblogic-enterprise-bean>
		<ejb-name>com_liferay_documentlibrary_service_ejb_DLLocalServiceEJB</ejb-name>
		<reference-descriptor>
			<resource-description>
				<res-ref-name>mail/MailSession</res-ref-name>
				<jndi-name>mail/MailSession</jndi-name>
			</resource-description>
		</reference-descriptor>
		<enable-call-by-reference>true</enable-call-by-reference>
		<local-jndi-name>ejb/liferay/DLLocalServiceHome</local-jndi-name>
	</weblogic-enterprise-bean>
	<weblogic-enterprise-bean>
		<ejb-name>com_liferay_documentlibrary_service_ejb_DLServiceEJB</ejb-name>
		<reference-descriptor>
			<resource-description>
				<res-ref-name>mail/MailSession</res-ref-name>
				<jndi-name>mail/MailSession</jndi-name>
			</resource-description>
		</reference-descriptor>
		<enable-call-by-reference>true</enable-call-by-reference>
		<jndi-name>com_liferay_documentlibrary_service_ejb_DLServiceEJB</jndi-name>
	</weblogic-enterprise-bean>
</weblogic-ejb-jar>