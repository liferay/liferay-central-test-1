<?xml version="1.0"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="com.liferay.documentlibrary.service.DLLocalService.enterprise" class="com.liferay.portal.spring.ejb.LocalSessionFactoryBean" lazy-init="true">
		<property name="businessInterface">
			<value>com.liferay.documentlibrary.service.DLLocalService</value>
		</property>
		<property name="jndiName">
			<value>ejb/liferay/DLLocalServiceHome</value>
		</property>
	</bean>
	<bean id="com.liferay.documentlibrary.service.DLLocalServiceFactory" class="com.liferay.documentlibrary.service.DLLocalServiceFactory" lazy-init="true">
		<property name="service">
			<ref bean="com.liferay.documentlibrary.service.DLLocalService.enterprise" />
		</property>
	</bean>
	<bean id="com.liferay.documentlibrary.service.DLService.enterprise" class="com.liferay.portal.spring.ejb.RemoteSessionFactoryBean" lazy-init="true">
		<property name="businessInterface">
			<value>com.liferay.documentlibrary.service.DLService</value>
		</property>
		<property name="jndiName">
			<value>com_liferay_documentlibrary_service_ejb_DLServiceEJB</value>
		</property>
	</bean>
	<bean id="com.liferay.documentlibrary.service.DLServiceFactory" class="com.liferay.documentlibrary.service.DLServiceFactory" lazy-init="true">
		<property name="service">
			<ref bean="com.liferay.documentlibrary.service.DLService.enterprise" />
		</property>
	</bean>
</beans>