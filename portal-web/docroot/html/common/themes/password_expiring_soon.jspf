<c:if test="<%= themeDisplay.isSignedIn() %>">

	<%
		boolean passwordExpiringSoon = UserLocalServiceUtil.isPasswordExpiringSoon(user);
	%>

	<c:if test="<%= passwordExpiringSoon %>">
		<aui:script use="liferay-passwordexpiringsoon">
			Liferay.PasswordExpiringSoon = new Liferay.PasswordExpiringSoon();
		</aui:script>
	</c:if>
</c:if>