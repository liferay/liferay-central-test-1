				<select <%= disabled ? "disabled" : "" %> id="<%= monthParam %>" name="<%= monthParam %>">
					<c:if test="<%= monthNullable %>">
						<option value=""></option>
					</c:if>
		
					<%
					for (int i = 0; i < months.length; i++) {
					%>
		
						<option <%= (monthValue == monthIds[i]) ? "selected" : "" %> value="<%= monthIds[i] %>"><%= months[i] %></option>
		
					<%
					}
					%>
		
				</select>
