		<layout-template id="2_columns_iii" name="2 Columns (70/30)">
			<template-path>/html/layouttpl/custom/2_columns_iii.tpl</template-path>
		</layout-template>
		<layout-template id="3_columns" name="3 Columns">
			<template-path>/html/layouttpl/custom/3_columns.tpl</template-path>
		</layout-template>
		<layout-template id="1_2_1_columns" name="1-2-1 Columns">
			<template-path>/html/layouttpl/custom/1_2_1_columns.tpl</template-path>
		</layout-template>
		<layout-template id="2_2_columns" name="2-2 Columns">
			<template-path>/html/layouttpl/custom/2_2_columns.tpl</template-path>
		</layout-template>
	</custom>
</layout-templates>