<definition>
	<command name="click">
		<case locator-key="CATEGORIES_MOVE_TO_THE_RECYCLE_BIN">
			<execute function="Click#valueClickAtAndWait" value1="Move to the Recycle Bin" />
		</case>
		<case locator-key="THREADS_MOVE_TO_THE_RECYCLE_BIN">
			<execute function="Click#valueClickAtAndWait" value1="Move to the Recycle Bin" />
		</case>
	</command>

	<command name="isElementPresent">
		<case locator-key="THREADS_SELECT_ALL">
			<execute function="IsElementPresent#isVisible" />
		</case>
	</command>
</definition>