<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 4.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_4_0_0.dtd">

<service-builder root-dir=".." package-path="com.liferay.portlet">
	<portlet name="Admin" short-name="Admin" />
	<entity name="AdminConfig" local-service="false" remote-service="true">

		<!-- PK fields -->

		<column name="configId" type="String" primary="true" />

		<!-- Audit fields -->

		<column name="companyId" type="String" />

		<!-- Other fields -->

		<column name="type" type="String" />
		<column name="name" type="String" />
		<column name="config" type="String" />

		<!-- Finder methods -->

		<finder name="CompanyId" return-type="Collection">
			<finder-column name="companyId" />
		</finder>
		<finder name="C_T" return-type="Collection">
			<finder-column name="companyId" />
			<finder-column name="type" />
		</finder>

		<!-- References -->

		<reference package-path="com.liferay.portal" entity="User" />
	</entity>
</service-builder>