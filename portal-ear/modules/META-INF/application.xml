<?xml version="1.0"?>
<!DOCTYPE application PUBLIC "-//Sun Microsystems, Inc.//DTD J2EE Application 1.3//EN" "http://java.sun.com/dtd/application_1_3.dtd">

<application>
	<display-name>Liferay Enterprise Portal</display-name>
	<module>
		<ejb>counter-ejb.jar</ejb>
	</module>
	<module>
		<ejb>documentlibrary-ejb.jar</ejb>
	</module>
	<module>
		<ejb>lock-ejb.jar</ejb>
	</module>
	<module>
		<ejb>mail-ejb.jar</ejb>
	</module>
	<module>
		<ejb>portal-ejb.jar</ejb>
	</module>
	<module>
		<web>
			<web-uri>cms-web.war</web-uri>
			<context-root>/cms-web</context-root>
		</web>
	</module>
	<module>
		<web>
			<web-uri>laszlo-web.war</web-uri>
			<context-root>/laszlo-web</context-root>
		</web>
	</module>
	<module>
		<web>
			<web-uri>portal-web.war</web-uri>
			<context-root>/</context-root>
		</web>
	</module>
	<module>
		<web>
			<web-uri>tunnel-web.war</web-uri>
			<context-root>/tunnel-web</context-root>
		</web>
	</module>
	<security-role>
		<role-name>users</role-name>
	</security-role>
</application>